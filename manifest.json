{
  "manifest_version": 3,
  "name": "Shinigami Scythe",
  "description": "Clean up your webfeeds.",
  "version": "1.0",
  "action": {
    "default_icon": "kama_icon.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*.twitter.com/*",
        "https://*.x.com/*",
        "https://*.youtube.com/*"
      ]
    }
  ]
}